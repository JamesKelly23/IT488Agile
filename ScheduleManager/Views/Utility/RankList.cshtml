@{
    ViewData["Title"]="Rank List";
    foreach (Rank theRank in ViewBag.RankList)
    {
        <div class="item-box">
            <p><h4>Rank @theRank.ID</h4></p>
            <p><h3>@theRank.Title</h3></p>
        </div>
    }
}