@model ScheduleManager.Models.EmployeeTester
<div class="text-center">
    <h1>Employee Information</h1>
</div>
<hr />
<div class="text-center"> 
    <h2>General</h2>
    <br />
</div>
<div>
    <dl class="row">
        <dt class="col-sm-2">
            First Name
        </dt>
        <dd class="col-sm-10">
            @Html.DisplayFor(model => model.FirstName)
        </dd>
    </dl>
    <dl class="row">
        <dt class="col-sm-2">
            Last Name
        </dt>                                                                                                                
         <dd class="col-sm-10">
            @Html.DisplayFor(model => model.LastName)
         </dd>
    </dl>
    <dl class="row">
         <dt class="col-sm-2">
             Rank
         </dt>
         <dd class="col-sm-10">
             @Html.DisplayFor(model => model.RankID)
         </dd>
    </dl>
    <dl class="row">
         <dt class="col-sm-2">
             Date of Birth
         </dt>
         <dd class="col-sm-10">
             @Html.DisplayFor(model => model.DOB)
         </dd>
    </dl>
</div>
<div class="text-center">
    <h2>Contact Information</h2>
</div>
<div>
    <dl class="row">
        <dt class="col-sm-2">
            Email
        </dt>
        <dd class="col-sm-10">
            @Html.DisplayFor(model => model.Email)
        </dd>
    </dl>
    <dl class="row">
        <dt class="col-sm-2">
            Phone
        </dt>
        <dd class="col-sm-10">
            @Html.DisplayFor(model => model.Phone)
        </dd>
    </dl>
</div>
<div class="text-center">
    <h2>Account Info</h2>
</div>
<div>
    <dl class="row">
        <dt class="col-sm-2">
            User Name
        </dt>
        <dd class="col-sm-10">
            @Html.DisplayFor(model => model.DOB)
        </dd>
    </dl>
    <dl class="row">
        <dt class="col-sm-2">
            Password
        </dt>
        <dd class="col-sm-10">
            @Html.DisplayFor(model => model.Password)
        </dd>
    </dl>
</div>
